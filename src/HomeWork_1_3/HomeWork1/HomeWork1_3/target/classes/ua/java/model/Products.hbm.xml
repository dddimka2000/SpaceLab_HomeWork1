<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.example.hibernate.products.Products" table="user_details">
        <id name="Product_id" column="id"></id>
        <property name="Productscol" column="Productscol"></property>
        <property name="Price" column="Price"></property>
    </class>

</hibernate-mapping>